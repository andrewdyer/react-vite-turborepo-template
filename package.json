{
  "name": "react-vite-turborepo-template",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": "22.20.0"
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md}'",
    "lint": "turbo lint",
    "preview": "turbo preview",
    "test": "turbo run test",
    "test:coverage": "turbo run test -- --coverage",
    "e2e": "turbo run e2e",
    "prepare": "husky install"
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.8"
  }
}
